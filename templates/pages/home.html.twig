{% extends '/layouts/base.html.twig' %}

{% block title %} {{ controller_name }} - {{ parent() }} {% endblock %}

{% block main %}
  <main class="c-main" role="main">

    <section class="c-main__container container">

      <div class="c-main__row row">
        <div class="c-main__col col text-center">

          {% for message in app.flashes('success') %}
            <div class="alert alert-success">
              {{ message }}
            </div>
          {% endfor %}

          <p><img src="{{ asset('build/images/sfff4.png') }}" class="img-fluid" alt=""></p>
        </div>
      </div>

    </section>

  </main>
{% endblock %}
